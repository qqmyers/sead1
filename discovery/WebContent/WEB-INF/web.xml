<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name></display-name>
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  
    <filter>
    <filter-name>AuthFilter</filter-name>
    <filter-class>
       org.sead.acr.common.auth.AuthFilter
    </filter-class>
    <init-param>
      <param-name>PropertiesFileName</param-name>
      <param-value>/brandedrepository.properties</param-value>
    </init-param>
    <init-param>
      <param-name>LoginPage</param-name>
      <param-value>/login</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>AuthFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>org.sead.acr.brandedrepository.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Home</servlet-name>
    <servlet-class>org.sead.acr.brandedrepository.Home</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ContentPage</servlet-name>
    <servlet-class>org.sead.acr.brandedrepository.Content</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContentPage</servlet-name>
    <url-pattern>/contents</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>GetPublishedCollections</servlet-name>
    <servlet-class>org.sead.acr.brandedrepository.GetPublishedCollections</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetPublishedCollections</servlet-name>
    <url-pattern>/GetPublishedCollections</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>CollectionContents</servlet-name>
    <servlet-class>org.sead.acr.brandedrepository.CollectionContents</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CollectionContents</servlet-name>
    <url-pattern>/GetContents</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetBibliographicInfo</servlet-name>
    <servlet-class>org.sead.acr.brandedrepository.GetBibliographicInfo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetBibliographicInfo</servlet-name>
    <url-pattern>/GetBiblio</url-pattern>
  </servlet-mapping>
  
  
</web-app>